<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
  <Id>99ceae05-b123-44cb-b88a-e06ceab657c7</Id>
  <Version>0.0.3.0</Version>
  <ProviderName>Jakub Pecanka</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="InkPilot"/>
  <Description DefaultValue="InkPilot for Microsoft Word"/>
  <IconUrl DefaultValue="https://pecanka.net/addin/favicon.ico"/>
  <SupportUrl DefaultValue="https://github.com/pecanka/InkPilot"/>

  <Hosts>
    <Host Name="Document"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="WordApi" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://pecanka.net/addin/taskpane.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>
</OfficeApp>